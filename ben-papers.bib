  keywords = {parallel execution,Provenance,Scientific workflows},
  file = {Full Text PDF:/Users/ben/Documents/zotero/storage/HSIACIB6/Ogasawara et al. - 2013 - Chiron a parallel engine for algebraic scientific.pdf:application/pdf;Snapshot:/Users/ben/Documents/zotero/storage/64U8TVHD/full.html:text/html}
